pub builtin def @exit(code: num): noreturn
   #[[internal]]
end

pub builtin def @panic{T}(msg: T): noreturn
  #[[internal]]
end

pub builtin def @string(val: any): str
  #[[internal]]
end

pub builtin def assert(arg: bool, msg: str): void
   #[[internal]]
end

pub builtin def print(args*: any): void
  #[[internal]]
end

pub builtin def println(args*: any): void
  #[[internal]]
end
